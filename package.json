{
    "name": "fvtt-module-dnd5e-spell-lists",
    "version": "1.0.0",
    "description": "Extends the D&D 5e spells tab to allow for creating and switching between different lists of prepared spells.",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsc && vite build",
        "stage": "tsc && npm run clean && vite build --mode stage",
        "dev": "tsc && npm run clean && vite build --mode development",
        "clean": "tsx ./build/clean.ts",
        "foundry": "tsx ./build/start-foundry.ts",
        "hot": "vite serve",
        "rename-module": "tsx ./build/rename-module.ts",
        "watch": "tsc && npm run clean && vite build --watch --mode development",
        "link": "tsx ./build/link-foundry.ts",
        "update-types": "tsx ./build/update-pf2e-types.ts && tsx ./build/copy-pf2e-types.ts",
        "test": "jest",
        "lint": "npm run lint:ts && npm run lint:json && npm run prettier:scss",
        "lint:ts": "eslint ./src --ext .ts",
        "prettier:scss": "prettier --check src/styles",
        "lint:json": "eslint ./static --ext .json --rule \"linebreak-style: error\"",
        "lint:fix": "eslint ./src --ext .ts --fix && prettier --write src/styles",
        "prepare": "husky"
    },
    "author": "JG (https://github.com/jagoe)",
    "license": "MIT",
    "engines": {
        "node": ">=20.15.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@eslint/js": "^9.26.0",
        "@eslint/json": "^0.13.2",
        "@pixi/basis": "7.4.3",
        "@pixi/graphics-smooth": "1.1.1",
        "@pixi/particle-emitter": "5.0.8",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@types/fs-extra": "^11.0.4",
        "@types/glob": "^8.1.0",
        "@types/jest": "^30.0.0",
        "@types/jquery": "^3.5.32",
        "@types/jsdom": "^27.0.0",
        "@types/luxon": "^3.6.2",
        "@types/node": "^24.9.2",
        "@types/prompts": "^2.4.9",
        "@types/showdown": "^2.0.6",
        "@types/simple-peer": "^9.11.8",
        "@types/sortablejs": "^1.15.8",
        "@types/tooltipster": "^0.0.35",
        "@types/uuid": "^10.0.0",
        "@types/yaireo__tagify": "^4.27.0",
        "@typescript-eslint/eslint-plugin": "^8.32.0",
        "@typescript-eslint/parser": "^8.32.0",
        "classic-level": "^3.0.0",
        "es-jest": "^2.1.0",
        "eslint": "^9.26.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-jest": "^29.0.1",
        "eslint-plugin-prettier": "^5.4.1",
        "fs-extra": "^11.3.0",
        "gsap": "^3.13.0",
        "handlebars": "4.7.8",
        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "jest-each": "^30.2.0",
        "js-angusj-clipper": "^1.3.1",
        "jsdom": "^27.1.0",
        "node-glob": "^1.2.0",
        "peggy": "^5.0.6",
        "pixi.js": "7.4.3",
        "prettier": "^3.5.3",
        "prettier-plugin-svelte": "^3.4.0",
        "prompts": "^2.4.2",
        "prosemirror-collab": "^1.3.1",
        "prosemirror-commands": "^1.7.1",
        "prosemirror-dropcursor": "^1.8.2",
        "prosemirror-gapcursor": "^1.3.2",
        "prosemirror-history": "^1.4.1",
        "prosemirror-inputrules": "^1.5.0",
        "prosemirror-keymap": "^1.2.2",
        "prosemirror-model": "^1.25.1",
        "prosemirror-schema-list": "^1.5.1",
        "prosemirror-state": "^1.4.3",
        "prosemirror-tables": "^1.7.1",
        "prosemirror-transform": "^1.10.4",
        "prosemirror-view": "^1.39.2",
        "sass": "^1.89.1",
        "socket.io": "4.8.1",
        "socket.io-client": "4.8.1",
        "svelte-preprocess": "^6.0.3",
        "tsx": "^4.19.4",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.32.0",
        "vite": "^7.1.12",
        "vite-plugin-checker": "^0.11.0",
        "vite-plugin-static-copy": "^3.0.0",
        "vite-tsconfig-paths": "^5.1.4",
        "yargs": "^18.0.0"
    }
}
